option types_output = "./out/unions"

type Union = u8 | string? | unknown

type GenUnion<A, B> = A | B
type FilledUnion = GenUnion<u8, string>

type foo = union "Tag" {
	First {},
	Second {}
}
type bar = union "Tag" {
	Third {},
	Fourth {}
}
type quax = struct {
	field: u8
}

type foo_bar_quax = u8 | foo | bar | quax

type model_instance = Instance<"Model"> | Instance
type button_enum = Enum<"Button"> | Enum

type state =
    "Idle"
    | "Running"
    | "Jumping"
    | "Falling"