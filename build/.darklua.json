{
  "bundle": {
    "excludes": [
      "@lune/**"
    ],
    "modules_identifier": "__DARKLUA_BUNDLE_MODULES",
    "require_mode": {
      "name": "luau"
    }
  },
  "generator": "readable",
  "rules": [
    "remove_types",
    "compute_expression",
    "remove_unused_if_branch",
    {
      "identifier": "RELEASE",
      "rule": "inject_global_value",
      "value": true
    },
    {
      "identifier": "VERSION",
      "rule": "inject_global_value",
      "value": "0.18.7"
    }
  ]
}